<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>Shoppe - <%= title %></title>

  <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/bootstrap/3.2.0/css/bootstrap.min.css">
  <link rel="stylesheet" type="text/css" href="/static/css/main.css">
  <% if(typeof css !== 'undefined') { %>
    <% for(var i = 0; i < css.length; i++) { %>
      <link rel="stylesheet" type="text/css" href="/static/css/<%= css[i] %>.css">
    <% } %>
  <% } %>
</head>
<body>
<nav class="navbar navbar-inverse navbar-static-top" role="navigation">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="/">Shoppe</a>
      <a class="navbar-brand" href="/cart"><span class="glyphicon glyphicon-shopping-cart"></span></a>
    </div>
  </div>
</nav>
